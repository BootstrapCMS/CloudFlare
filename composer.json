{
    "name": "graham-campbell/cloudflare",
    "description": "CloudFlare Provides A Simple CloudFlare Module For Laravel 4.1/4.2",
    "keywords": ["laravel", "framework", "cloudflare api", "cloudflare", "CloudFlare", "Laravel CloudFlare", "Laravel-CloudFlare", "Graham Campbell", "GrahamCampbell"],
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Graham Campbell",
            "email": "graham@mineuk.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/GrahamCampbell/asset"
        }
    ],
    "require": {
        "php": ">=5.4.7",
        "illuminate/routing": "~4.1",
        "illuminate/support": "~4.1",
        "illuminate/view": "~4.1",
        "lightgear/asset": "1.4",
        "graham-campbell/core": "~1.0",
        "graham-campbell/cloudflare-api": "0.6.*"
    },
    "require-dev": {
        "graham-campbell/testbench": "~1.0"
    },
    "suggest": {
        "graham-campbell/navigation": "Easily add a navigation bar."
    },
    "autoload": {
        "psr-4": {
            "GrahamCampbell\\CloudFlare\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GrahamCampbell\\Tests\\CloudFlare\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.2-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
