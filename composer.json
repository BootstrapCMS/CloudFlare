{
    "name": "graham-campbell/cloudflare",
    "description": "CloudFlare Provides A Simple CloudFlare Module For Laravel 4.1",
    "keywords": ["laravel", "framework", "cloudflare api", "cloudflare", "CloudFlare", "Laravel CloudFlare", "Laravel-CloudFlare", "Graham Campbell", "GrahamCampbell"],
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Graham Campbell",
            "email": "graham@mineuk.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/GrahamCampbell/asset"
        }
    ],
    "require": {
        "php": ">=5.4.7",
        "laravel/framework": "4.1.*",
        "symfony/config": "2.4.*",
        "symfony/event-dispatcher": "2.4.*",
        "symfony/filesystem": "2.4.*",
        "lightgear/asset": "1.4",
        "graham-campbell/core": "0.3.*",
        "graham-campbell/viewer": "0.2.*",
        "graham-campbell/cloudflare-api": "0.4.*"
    },
    "require-dev": {
        "graham-campbell/testbench": "0.3.*"
    },
    "autoload": {
        "psr-4": {
            "GrahamCampbell\\CloudFlare\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GrahamCampbell\\Tests\\CloudFlare\\": "tests/"
        }
    },
    "suggest": {
        "graham-campbell/navigation": "Easily add a navigation bar."
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
